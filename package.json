    "start:trpc": "pnpm --filter '@navaja/trpc' start"
  },
  "dependencies": {
    "firebase": "^10.7.1",
    "superjson": "^2.2.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "eslint": "^9.10.0",
    "eslint-config-prettier": "^9.1.0",
    "typescript": "~5.5.4",
    "typescript-eslint": "^8.4.0"
  }
}
