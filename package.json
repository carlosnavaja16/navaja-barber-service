{
  "name": "barber-service",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4201",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "push": "rm -rf dist && rm -rf ./functions/lib && eslint --fix ./ && prettier -w --single-quote ./ && git add . && git commit -m \"$(date)\" && git push",
    "deploy:functions": "rm -rf ./functions/lib && firebase deploy --only functions && rm -rf ./functions/lib",
    "deploy:hosting": "rm -rf dist && ng build && firebase deploy --only hosting && rm -rf dist",
    "emulate:functions": "tsc ./functions/*.ts --outdir ./functions/lib && firebase emulators:start --only functions",
    "format": "prettier -w --single-quote ./",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.6",
    "@angular/cdk": "^16.2.5",
    "@angular/common": "^16.2.6",
    "@angular/compiler": "^16.2.6",
    "@angular/core": "^16.2.6",
    "@angular/fire": "^7.6.1",
    "@angular/forms": "^16.2.6",
    "@angular/localize": "^16.2.6",
    "@angular/material": "^16.2.5",
    "@angular/platform-browser": "^16.2.6",
    "@angular/platform-browser-dynamic": "^16.2.6",
    "@angular/router": "^16.2.6",
    "@ng-bootstrap/ng-bootstrap": "^15.1.1",
    "@ng-icons/core": "^25.3.1",
    "@ng-icons/heroicons": "^25.3.1",
    "@popperjs/core": "^2.11.8",
    "@types/bootstrap": "^5.2.7",
    "@types/luxon": "^3.4.0",
    "bootstrap": "^5.3.2",
    "bootstrap-icons": "^1.11.1",
    "firebase": "~9.23.0",
    "firebase-functions": "^4.5.0",
    "googleapis": "^126.0.1",
    "luxon": "^3.4.3",
    "ng": "^0.0.0",
    "rxfire": "^6.0.0",
    "rxjs": "~7.8.1",
    "tslib": "^2.6.2",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.3",
    "@angular-eslint/builder": "16.2.0",
    "@angular-eslint/eslint-plugin": "16.2.0",
    "@angular-eslint/eslint-plugin-template": "16.2.0",
    "@angular-eslint/schematics": "16.2.0",
    "@angular-eslint/template-parser": "16.2.0",
    "@angular/cli": "~16.2.3",
    "@angular/compiler-cli": "^16.2.6",
    "@ng-icons/bootstrap-icons": "^25.4.0",
    "@types/jasmine": "~4.3.6",
    "@typescript-eslint/eslint-plugin": "6.7.3",
    "@typescript-eslint/parser": "6.7.3",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "jasmine-core": "~5.1.1",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.3",
    "typescript": "~5.1.6"
  },
  "engines": {
    "node": "18"
  },
  "main": "functions/lib/index.js"
}
